Algoritmo "exercicio1"

Var

   x, y, z, a, b : real


Inicio

   escreva("Digite a largura: ")
   leia(x)
   escreva("Digite o comprimento: ")
   leia(y)
   escreva("Digite o valor do metro quadrado: ")
   leia(z)
   
   a <- x * y
   b <- a * z
   
   escreval("Valor da area do terreno: ", a:5:2)
   escreval("Valor preço do terreno: ", b:5:2)


Fimalgoritmo